<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CheckButton">
        <attr name="checkBackColor" format="color"></attr>
        <attr name="checkTextColor" format="color"></attr>
        <attr name="checkImage" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="UIButton">
        <attr name="backColor" format="color"></attr>
        <attr name="backImage" format="reference"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="backColorSelected" format="color"></attr>
        <attr name="backImageSelected" format="reference"></attr>
        <attr name="textColorSelected" format="color"></attr>
        <attr name="shap" format="integer"></attr>
        <attr name="fillset" format="boolean"></attr>
        <attr name="radius" format="float"></attr>
    </declare-styleable>

    <declare-styleable name="SSQInformationShow">
        <attr name="titlestyle" format="reference"></attr>
        <attr name="titleshow" format="string"></attr>
        <!--<attr name="titlefontsize" format="dimension"></attr>-->
        <attr name="titlecolor" format="color"></attr>

        <attr name="textstyle" format="reference"></attr>
        <attr name="textshow" format="string"></attr>
        <attr name="textcolor" format="color"></attr>

        <attr name="textfontsize" format="dimension"></attr>

    </declare-styleable>

</resources>
